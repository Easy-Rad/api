{
	"scripts": {
		"watch:webpack": "webpack --watch",
		"watch:registrar-numbers": "tailwindcss -i frontend/registrar-numbers.css -o app/static/registrar-numbers.css -mw",
		"watch:reports": "tailwindcss -i frontend/reports.css -o app/static/reports.css -mw",
		"build:webpack": "webpack",
		"build:registrar-numbers": "tailwindcss -i frontend/registrar-numbers.css -o app/static/registrar-numbers.css -m",
		"build:reports": "tailwindcss -i frontend/reports.css -o app/static/reports.css -m",
		"build:all": "pnpm run build:webpack && pnpm run build:registrar-numbers && pnpm run build:reports"
	},
	"devDependencies": {
		"@tailwindcss/cli": "^4.1.17",
		"tailwindcss": "^4.1.17",
		"webpack": "^5.102.1",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"apexcharts": "^5.3.6",
		"flowbite": "^3.1.2",
		"simple-datatables": "^10.2.0"
	}
}
